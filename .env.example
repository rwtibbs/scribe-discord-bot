# Discord Bot Configuration
DISCORD_BOT_TOKEN=your_discord_bot_token_here
DISCORD_CLIENT_ID=your_discord_client_id_here

# AWS Configuration
AWS_ACCESS_KEY_ID=your_aws_access_key_id_here
AWS_SECRET_ACCESS_KEY=your_aws_secret_access_key_here
AWS_REGION=us-east-2
AWS_ENVIRONMENT=DEV

# TabletopScribe API Configuration
USER_POOL_CLIENT_ID_DEV=your_cognito_user_pool_client_id_here
APPSYNC_API_KEY=your_appsync_api_key_here

# Database Configuration
DATABASE_URL=postgresql://user:password@localhost:5432/tabletopscribe_bot

# Application Configuration (Optional)
NODE_ENV=development
PUBLIC_URL=http://localhost:5000

# Notes:
# - Never commit this file with real values to Git
# - Copy this to .env and fill in your actual credentials
# - For Railway deployment, set these as environment variables in the dashboard
# - PUBLIC_URL is required for production deployments (use your Railway URL)
# - AWS_ENVIRONMENT can be DEV or DEVSORT depending on your setup
